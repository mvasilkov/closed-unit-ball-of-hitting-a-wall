{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2020",
    "moduleResolution": "node",
    "declaration": false,
    "outDir": "./out",
    "rootDir": "./typescript",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "newLine": "LF"
  },
  "references": [
    {
      "path": "./node_modules/natlib"
    }
  ]
}
